{
  "name": "neutrino-webextension-mv3",
  "version": "1.2.5",
  "description": "Neutrino 9 preset for WebExtension development with hot reload and framework devtools. only manifest v3 supported.",
  "keywords": [
    "neutrino",
    "preset",
    "middleware",
    "webextension",
    "extension",
    "devtool"
  ],
  "author": "CRIMX",
  "homepage": "https://github.com/crimx/neutrino-webextension#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crimx/neutrino-webextension.git"
  },
  "bugs": {
    "url": "https://github.com/crimx/neutrino-webextension/issues"
  },
  "engines": {
    "node": "^8.10 || >=10",
    "npm": ">=5.4.0",
    "yarn": ">=1.2.1"
  },
  "main": "lib/index.js",
  "bin": {
    "neutrino-webextension-zip": "./lib/zip.js"
  },
  "files": [
    "livereload/**/*",
    "lib/**/*",
    "template.dev.ejs",
    "template.ejs",
    "yarn.lock"
  ],
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --mode development --open",
    "build": "NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "test": "jest --coverage",
    "test:CI": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "deepmerge": "^3.3.0",
    "fs-extra": "^9.0.0",
    "globby": "^10.0.1",
    "open": "^7.0.4",
    "webextensions-emulator": "^1.2.6",
    "webpack-target-webextension-v4": "0.2.2",
    "yargs-parser": "^18.1.0"
  },
  "devDependencies": {
    "@neutrinojs/copy": "^9.0.0-rc.3",
    "@neutrinojs/react": "^9.0.0-rc.3",
    "@neutrinojs/standardjs": "^9.0.0-rc.3",
    "coveralls": "^3.0.4",
    "eslint": "^6.7.2",
    "jest": "^24.8.0",
    "neutrino": "^9.0.0-rc.3",
    "react": "^16",
    "react-dom": "^16",
    "react-hot-loader": "^4",
    "webextension-polyfill": ">=0.4.0",
    "webpack": "^4",
    "webpack-cli": "^3",
    "webpack-dev-server": "^3"
  },
  "peerDependencies": {
    "neutrino": "^9.0.0-rc.3",
    "webextension-polyfill": ">=0.4.0",
    "webpack": "4.33.0",
    "webpack-cli": "^3"
  }
}
